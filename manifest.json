{
  "name": "VismaBareBedre",
  "version": "1.0",
  "description": "Utvider funksjonaliteteten til visma inschool",
  "permissions": ["declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/Vismabarebedre16.png",
      "32": "images/Vismabarebedre32.png"
    }
  },
  "icons": {
    "16": "images/Vismabarebedre16.png",
    "32": "images/Vismabarebedre32.png"
  },
  "manifest_version": 2
}